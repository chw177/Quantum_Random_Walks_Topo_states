       ",", "a", ",", "temp", ",", "d", ",", "rotation1", ",", "rotation2"}], 
      "}"}], ",", " ", 
     RowBox[{
      RowBox[{"boundarylength", "=", "0.01"}], ";", " ", " ", 
      RowBox[{"Initialup", "=", " ", 
       RowBox[{"N", "[", 
        RowBox[{"Cos", "[", "iangle", "]"}], "]"}]}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{"Not", " ", "Needed", " ", "Normalised"}], "*)"}], " ", 
      RowBox[{"Initialdown", "=", " ", 
       RowBox[{"N", "[", " ", 
