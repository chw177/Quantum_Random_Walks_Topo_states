        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"l", ",", "n"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{
            RowBox[{"4", "n"}], "+", "1"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "2"}], "}"}]}], "]"}]}], ";", " ", " ", 
      RowBox[{"temp", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{
            RowBox[{"4", "n"}], "+", "1"}]}], "}"}], ",", 
